<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="rhynix.eliud.androidanimation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".animationactivities.LaunchRocketValueAnimatorAnimationActivity"/>
        <activity android:name=".animationactivities.AccelerateRocketAnimationActivity"/>
        <activity android:name=".animationactivities.RotateRocketAnimationActivity"/>
        <activity android:name=".animationactivities.LaunchRocketObjectAnimatorAnimationActivity"/>
        <activity android:name=".animationactivities.ColorAnimationActivity"/>
        <activity android:name=".animationactivities.LaunchAndSpinAnimatorSetAnimatorActivity"/>
        <activity android:name=".animationactivities.LaunchAndSpinViewPropertyAnimatorAnimationActivity"/>
        <activity android:name=".animationactivities.XmlAnimationActivity"/>
        <activity android:name=".animationactivities.FlyThereAndBackAnimationActivity"/>
       <activity android:name=".animationactivities.WithListenerAnimationActivity"/>
        <activity android:name=".animationactivities.FlyWithDogeAnimationActivity"/>
    </application>

</manifest>